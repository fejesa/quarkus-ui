{
  "name": "primebox",
  "version": "0.0.0",
  "_comments": {
    "scripts": {
      "start": "Starts the Angular development server",
      "watch": "Builds the project in watch mode with the development configuration",
      "dev": "Runs Orval (API generator) and then Angular CLI",
      "build": "Runs Orval and then builds the project for production"
    },
    "dependencies": "Main dependencies required for the application to run",
    "devDependencies": "Development tools and libraries for building and testing the application"
  },
  "scripts": {
    "start": "ng serve",
    "watch": "ng build --watch --configuration development",
    "dev": "orval --config ./orval.config.ts && ng",
    "build": "orval --config ./orval.config.ts && ng build",
    "serve:ssr:primebox": "node dist/primebox/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/platform-server": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@angular/ssr": "^19.0.6",
    "@primeng/themes": "^19.0.2",
    "express": "^4.18.2",
    "primeng": "^19.0.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.6",
    "@angular/cli": "^19.0.6",
    "@angular/compiler-cli": "^19.0.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "orval": "^7.3.0",
    "typescript": "~5.6.2"
  }
}